{
    "manifest_version": 3,
    "name": "AdjusTimer",
    "version": "3.0.0",
    "action": {
      "default_popup": "src/popup/index.html"
    },
    "content_scripts": [
      {
        "js": ["src/content/index.tsx"],
        "matches": ["https://www.google.com/*"]
      }
    ],
    "background": {
      "service_worker": "src/background/index.ts"
    },
    "host_permissions": [
      "<all_urls>"
    ]
}